                    "value": "create"
                  }
                ]
              },
              "renameOutput": true,
              "outputKey": "insert"
            },
            {
              "conditions": {
                "all": [
                  {
                    "field": "={{ $json.action }}",
                    "operation": "equals",
                    "value": "read"
                  }
                ]
              },
              "renameOutput": true,
              "outputKey": "find"
            },
            {
              "conditions": {
                "all": [
                  {
                    "field": "={{ $json.action }}",
                    "operation": "equals",
                    "value": "update"
                  }
                ]
              },
